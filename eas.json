{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
       "android": {
        "env": {
          "EAS_NO_ARTIFACTORY": "1",
          "GRADLE_OPTS": "-Xmx4096m -XX:MaxMetaspaceSize=1024m"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest",
        "env": {
          "EAS_NO_ARTIFACTORY": "1",
          "GRADLE_OPTS": "-Xmx4096m -XX:MaxMetaspaceSize=1024m"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "env": {
          "EAS_NO_ARTIFACTORY": "1",
          "GRADLE_OPTS": "-Xmx4096m -XX:MaxMetaspaceSize=1024m"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}